{
  "name": "skylark-ui-repeater",
  "description": "The skylark repeater widget",
  "version": "0.9.0",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/skylarkui/skylark-ui-repeater/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "change-version": "node grunt/change-version.js",
    "update-shrinkwrap": "npm shrinkwrap --dev && shx mv ./npm-shrinkwrap.json ./grunt/npm-shrinkwrap.json",
    "test": "grunt test"
  },
  "style": "dist/css/bootstrap.css",
  "less": "less/bootstrap.less",
  "main": "./dist/js/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylarkui/skylark-ui-repeater.git"
  },
  "bugs": {
    "url": "https://github.com/skylarkui/skylark-bs-repeater/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "del": "^1.2.1",
    "gulp": "^3.9.0",
    "gulp-amd-optimizer": "^0.3.4",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.7.1",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-requirejs": "^1.0.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.5.4",
    "gulp-usemin": "^0.3.11",
    "gulp-util": "^3.0.6",
    "run-sequence": "^2.1.0",
    "skylark-langx": "https://github.com/skylarklangx/skylark-langx#master",
    "skylark-utils": "https://github.com/skylarkutils/skylark-utils#master",
    "skylark-ui-swt": "https://github.com/skylarkui/skylark-ui-swt#master",
    "btoa": "~1.1.2",
    "glob": "~7.0.3",
    "markdown-it": "^7.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.2"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "dist",
    "fonts",
    "grunt",
    "js/*.js",
    "less/**/*.less",
    "Gruntfile.js",
    "LICENSE"
  ],
  "jspm": {
    "main": "js/bootstrap",
    "shim": {
      "js/bootstrap": {
        "deps": "jquery",
        "exports": "$"
      }
    },
    "files": [
      "css",
      "fonts",
      "js"
    ]
  }
}
